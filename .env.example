# OpenClaw Agent - Environment Variables
# =======================================
# Copy this file to .env and fill in your API keys

# RECOMMENDED: Groq (FREE - generous limits)
# Get your key at: https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# Alternative: Anthropic Claude (paid)
ANTHROPIC_API_KEY=

# Alternative: Google Gemini (free tier available)
GOOGLE_API_KEY=

# Optional: Premium search providers
# If not set, DuckDuckGo (free) will be used
SERPER_API_KEY=
TAVILY_API_KEY=
